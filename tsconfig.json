{
    "compilerOptions": {
        "target": "es2016",
        "module": "esnext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "removeComments": true,
        "skipLibCheck": true,
        "noEmit": true,
        "jsx": "react"
    },
    "include": ["src/**/*"]
}
